<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate
            android:fromDegrees="45"
            android:pivotX="-100%"
            android:pivotY="100%"
            android:toDegrees="45">
            <shape android:shape="rectangle">
                <size
                    android:width="20dp"
                    android:height="20dp" />
                <solid android:color="@color/white" />
            </shape>
        </rotate>
    </item>
</layer-list>