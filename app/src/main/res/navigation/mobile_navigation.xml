<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.wwwjf.wanandroidkt.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.wwwjf.wanandroidkt.ui.gallery.GalleryFragment"
        android:label="@string/menu_gallery"
        tools:layout="@layout/fragment_gallery" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.wwwjf.wanandroidkt.ui.slideshow.SlideshowFragment"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/fragment_slideshow" />

    <activity android:id="@+id/nav_main"
        android:name="com.wwwjf.wanandroidkt.MainActivity"/>

    <activity
        android:id="@+id/nav_jetpack_demo"
        android:name="com.wwwjf.jetpackdemo.navigation.navigationDemoActivity"/>

    <activity
        android:id="@+id/nav_ivy_demo"
        android:name="com.xianghe.ivy.ui.module.welcom.XWLauncherActivity"/>
</navigation>